{
  "name": "magicssian",
  "version": "0.1.8",
  "description": "A CSS Boilerplate",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "dependencies": {},
  "devDependencies": {
    "clean-css-cli": "^4.2.1",
    "html5-boilerplate": "^7.0.1",
    "html5-test-page": "^0.8.0",
    "postcss-calc": "^7.0.1",
    "postcss-cli": "^6.1.2",
    "postcss-color-function": "^4.0.1",
    "postcss-css-variables": "^0.12.0",
    "postcss-import": "^12.0.1",
    "version-bump-prompt": "^5.0.0",
    "watch": "^1.0.2"
  },
  "scripts": {
    "watch": "watch 'npm run build' ./src/scss_css",
    "postcss": "postcss src/scss_css/magicssian_draft.css -u postcss-import postcss-css-variables postcss-calc postcss-color-function --no-map -o docs/css/magicssian.css",
    "cleancss": "cleancss -O1 --format keep-breaks -o docs/css/magicssian.css docs/css/magicssian.css",
    "minifycss": "cleancss -O1 specialComments:1 -02 --format breaks:afterComment=on --source-map -o docs/css/magicssian.min.css docs/css/magicssian.css",
    "build": "npm run postcss && npm run cleancss && npm run minifycss",
    "vbump-patch": "bump patch package.json package-lock.json README.md src/scss_css/magicssian_draft.css src/scss_css/magicssian_variables.css",
    "dist": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeinalabMX/magicssian.git"
  },
  "keywords": [
    "CSS",
    "Biolerplate"
  ],
  "author": "hEy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CodeinalabMX/magicssian/issues"
  },
  "homepage": "https://codeinalabmx.github.io/magicssian/"
}
