{
  "name": "magicssian",
  "version": "0.1.0",
  "description": "A CSS Boilerplate",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "dependencies": {},
  "devDependencies": {
    "browser-sync": "^2.26.3",
    "clean-css-cli": "^4.2.1",
    "concurrently": "^4.1.0",
    "html5-boilerplate": "^7.0.1",
    "html5-test-page": "^0.8.0",
    "nodemon": "^1.18.10",
    "postcss-calc": "^7.0.1",
    "postcss-cli": "^6.1.2",
    "postcss-color-function": "^4.0.1",
    "postcss-contrast": "0.0.4",
    "postcss-css-variables": "^0.12.0",
    "postcss-custom-properties": "^8.0.9",
    "watch": "^1.0.2"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "bsync": "browser-sync start -p localhost:80/Codeinalab/Magicssian/CssBoilerplate/Master/docs/ -f *.html, css/*.css",
    "postcss": "postcss src/scss_css/magicssian_draft.css -u postcss-css-variables postcss-calc postcss-color-function --no-map -o docs/css/magicssian.css",
    "cleancss": "cleancss --source-map -o docs/css/magicssian.min.css docs/css/magicssian.css",
    "build": "npm run postcss && npm run cleancss",
    "watch": "watch \"npm run build\" src/scss_css",
    "dev": "concurrently \"npm run bsync\" \"npm run watch\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeinalabMX/magicssian.git"
  },
  "keywords": [
    "CSS",
    "Biolerplate"
  ],
  "author": "hEy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CodeinalabMX/magicssian/issues"
  },
  "homepage": "https://github.com/CodeinalabMX/magicssian#readme"
}
