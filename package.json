{
  "name": "magicssian",
  "version": "0.1.0",
  "description": "A CSS Boilerplate",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "dependencies": {},
  "devDependencies": {
    "browser-sync": "^2.26.3",
    "clean-css-cli": "^4.2.1",
    "html5-boilerplate": "^7.0.1",
    "html5-test-page": "^0.8.0",
    "nodemon": "^1.18.10",
    "postcss-cli": "^6.1.2"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "bsync": "browser-sync start -p localhost:80/Codeinalab/Magicssian/CssBoilerplate/Master/public_html/ -f *.html, css/*.css",
    "postcss": "postcss src/scss_css/magicssian.css --no-map -o public_html/css/magicssian.css",
    "cleancss": "cleancss --source-map -o public_html/css/magicssian.min.css public_html/css/magicssian.css",
    "build": "npm run postcss && npm run cleancss",
    "watch": "nodemon -e src/css -x npm run build",
    "dev": "npm run bsync && npm run watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeinalabMX/magicssian.git"
  },
  "keywords": [
    "CSS",
    "Biolerplate"
  ],
  "author": "hEy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CodeinalabMX/magicssian/issues"
  },
  "homepage": "https://github.com/CodeinalabMX/magicssian#readme"
}
